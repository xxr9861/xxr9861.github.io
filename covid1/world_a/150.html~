    var options_linechart = {
        title: 'Active Covid-19 cases for top ten countries (5 months ago to present day)',
        curveType: 'function',
        legend: { position: 'bottom' }
    };

    var linechart = new google.visualization.LineChart(document.getElementById('user_selected_chart'));

    linechart.draw(data_linechart, options_linechart);
    // now for the piecharts
    var data_piechart_today = google.visualization.arrayToDataTable([
	[ 'Country', 'Active Covid-19 cases ' ],
	    ['USA',2420137],['S__Korea',1044288],['Vietnam',837498],['Japan',837389],['Poland',792069],['Taiwan',725454],['Russia',676816],['Germany',595014],['Italy',427222],['France',411642]    ]);
    var data_piechart_past = google.visualization.arrayToDataTable([
	[ 'Country', 'Active Covid-19 cases' ],
	    ['USA',28677716],['S__Korea',477581],['Vietnam',185401],['Japan',893817],['Poland',732696],['Taiwan',1219],['Russia',2371348],['Germany',3179506],['Italy',1813274],['France',4981460]    ]);
    var options_piechart_today = {
	title: 'Distribution of active Covid-19 cases (Today)',
	legend: 'Active Covid-19 cases',
	chartArea: {width: 500, height: 700}

    };
    var options_piechart_past = {
	title: 'Distribution of active Covid-19 cases (5 months ago)',
	legend: 'Active Covid-19 cases',
	chartArea: {width: 500, height: 700}

    };
    
    var piechart_active_today = new google.visualization.PieChart(document.getElementById('piechart_active_today'));
    piechart_active_today.draw(data_piechart_today, options_piechart_today);
    var piechart_active_past = new google.visualization.PieChart(document.getElementById('piechart_active_past'));
    piechart_active_past.draw(data_piechart_past, options_piechart_past);
    

}
